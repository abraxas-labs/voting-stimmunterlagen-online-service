@model dynamic
<!DOCTYPE html>
<html>
    <head>
        <style>
            /* font */
            @@page {
                size: A4;
                margin: 0;
                font-size:10px;
            }
            html, body{
                -webkit-print-color-adjust: exact;
                margin: 0;
                font-family: Arial, Helvetica, sans-serif;
                font-size:10px;
                line-height: 1.2;
            }
            body{
                padding: 13mm 13mm 0 13mm;
            }
            * {
                font-size:10px;
            }
            .footer {
                position: fixed;
                bottom: 8mm;
                left: 13mm;
                width: 184mm; /* 210mm - 26mm */
            }
            .content-line{
                line-height: 1.5;
            }
            ul, ol {
                margin-left: 0;
                padding-left: 14px;
            }
            h1{
                font-size:22px;
                font-weight:bold;
                margin:0;
            }
            h2{
                font-size:14px;
                font-weight:bold;
                margin:0;
            }
            h3{
                font-size:11px;
                font-weight:bold;
                margin:0;
            }
            hr{
                border:none;
                border-bottom: 1px solid black;
            }

            /* style */
            .color-green{
                background-color: rgba(0, 153, 51, 0.2);
            }
            .content{
                padding:5px;
            }
            .title{
                background-color: rgba(0, 153, 51, 0.2);
                padding:5px;
                font-weight:bold;
            }
            .row{
                display:block;
            }
            .body-top{
                height:120mm; /* 151 -13*/
                display:flex;
                flex-direction: column;
            }
            .body-top > *:last-child {
                margin-bottom: 10px;
            }
            .no-page-break-inside{
                page-break-inside: avoid;
            }
            .page-break-after{
                page-break-after: always;
            }

            /* element */
            .printer-information{
                position: fixed;
                color:white;
                font-size:8px;
                top:1px;
                left:1px;
                font-family:Courier New;
            }
            .body-top .blocker{	
                flex:1;
            }

            /*** images ***/
            .arrow-down{
                height:6mm;
                width:6mm;
                background-image: url('data:image/png;base64, ');
                background-position: center center;
                background-repeat: no-repeat;
                background-size: contain;
            }

            /*** logo ***/
            .logo{
                position: absolute;
                top:7mm;
                left:17.5mm;
            }
            .logo img{
                height:20mm;
                width: auto;
            }

            /*** reciver address ***/
            .reciver-address{
                position:absolute;
                top:39mm;
                left: 22mm;
            }
            .reciver-address span{
                font-size:10pt;
            }

            /*** sender address ***/
            .sender-address{
                position: absolute;
                top: 73mm;
                left: 108mm;
                padding-left: 18mm;
                padding-top:26mm;
                border-top-color: rgba(0, 153, 51, 0.2);
                border-top-width: 2.5mm;
                border-style: solid none none none;
                width:71mm;
                height: 24mm ;
                word-wrap: break-word;
                max-width: 230px;
            }

            .sender-address span{
                font-size:10pt;
            }

            /*** birth-date***/
            .birth-date{
                position:absolute;
                top:30mm;
                right: 33mm;
            }

            /*** poll-title ***/
            .poll-title {
                font-weight: bold;
                margin: 10px 0 0 0;
            }
            .sra-title{
                margin-bottom: 10px;
            }

            /*** sign ***/
            .sign{
                width:49.8%;
                padding-right:10px;
            }
            .sign-content{
                margin-right: 10px;
            }
            .sign-line {
                border-bottom: solid 1px;
                height: 30pt;
            }

            /** .poll-date-title **/
            .poll-date-title{
                display: flex;
                border-bottom:solid 1px black;
            }
            .poll-date-title div{
                flex:1;
            }

            /*** text blocks ***/
            .text-blocks {
                display: flex;
                flex-direction: row;
                margin: 0px;
            }
            .text-blocks-column{
                display: flex;
                min-width:100px;
                flex-direction: column;
            }
            .text-blocks-column > div{
                flex: 1;
            }
            .text-block:not(.c50-50-50) {
                margin: -5px;
            }
            .text-blocks .content *, .text-blocks .content{
                min-height:1em; /*prince: <div><br></div> space fix*/
            }
            .text-blocks > div {
                flex: 1;
                margin: 0px;
            }
            .text-blocks:not(.c33-34-33) > div {
                margin: 5px;
            }
            .text-blocks.c66-34 > div:nth-child(1) {
            flex: 2;
            }
            .text-blocks.c66-34 > div:nth-child(2) {
            flex: 1;
            }
            .text-blocks.c34-66 > div:nth-child(1) {
            flex: 1;
            }
            .text-blocks.c34-66 > div:nth-child(2) {
            flex: 2;
            }

            /** e-voting **/
            .e-voting table{
                border-collapse: collapse;
                width:100%;
            }
            .e-voting td{
                padding: 3px 0px;
            }
            .e-voting th{
                font-size: 10pt;
                text-align: left;
            }

            /** page information**/
            .page-information{
                margin-top:3px;
                display: flex;
            }
            .page-information .info-content{
                flex:1;
            }

            /*** qrCood www.evote***/
            .qr-evoting{
                display: inline-block;
                width:35px;
                height:35px;
                background-image: url('data:image/png;base64,');
                background-position: center center;
                background-repeat: no-repeat;
                background-size: contain;
            }

            /*** deals ***/
            .deals{
                display:flex;
                margin:0 5px;
                padding: 7px 0;
                border-bottom: 1px solid black;
            }
            .deals > span{
                flex:1;
            }
            .deal-title{
                margin-bottom:5px;
            }

            /** Fingerprint **/
            .fingerprint{
                display:flex;
                gap:20px;
                margin-top:10px;
                margin-bottom:10px;
                font-weight:normal;
    			
				
            }
            .fingerprint label{
                display: flex;
                width: 300px;
            }

            .fingerprint-code{
                display:flex;
                flex-direction:column;
                justify-content:center;
                word-wrap: break-word;
                width: 300px;
            }

            .arrow-content{
                display: flex;
                justify-content: start;
                align-items: center;		
            }

            .arrow-structur{
                display: flex;
                justify-content:space-between;				
            }

            /** poll-base-data **/
            .poll-base-data{
                display:flex;
                max-width:100
            }
            .poll-base-data .poll-base-data-table{
                flex:2;
            }
            .poll-base-data .qr{
                flex:1;
                display: flex;
            }
            .qr-or{
                padding-top: 15px;
            }
            .poll-domain-of-influence{
                border-bottom: solid 1px black;
                font-weight:bold;
            }
            .turn-icon{
                width:7mm;
                height:7mm;
                background-image: url('data:image/svg+xml;base64,');
                background-position: center center;
                background-repeat: no-repeat;
                background-size: contain;
            }

            /***proporz ***/
            .proporz-item{
                display: inline-block;
                vertical-align: text-top;
                width: 18%;
                margin: 5px;
            }
            .proporz-item .code{
                text-align: right;
            }
            .proporz-item .content td{
                padding:3px;
            }
            .textRight{
                text-align:right;
            }

            /***** icons ****/
            .icon-star, .icon-triangle, .icon-pentagon{
                background-repeat: no-repeat;
                background-size: 20px;
                background-position: 7px;
                min-width: 35px;
                min-height: 20px;
            }
            .icon-triangle{
                background-image: url('data:image/png;base64,');

            }
            .icon-pentagon{
                background-image: url('data:image/png;base64,');

            }
            .icon-star{
                background-image: url('data:image/png;base64,');
            }
            .rhombus{
                font-size: large;
            }
        </style>
        <script>
            PDF.embedFonts = true;
        </script>
    </head>
    <body>
        @foreach (var voteElement in Model.votingCardDelivery.votingCardData){
            <div class="page-break-after">
                <div class="footer">
                    <div class="arrow-structur">
                        <div style="width:50%;"> </div>
                        <div  class="arrow-structur" style="width:50%;"> 
                            <div class="arrow-down"></div>
                            <div class="turn-icon"></div>
                        </div>
                    </div>
                </div>
                <div class="body-top">
                    @{
                        var personId = "";
                        var languageMap = new[] {"de", "fr", "it", "rm"};
                        dynamic nationality = null;
                        dynamic municipality = null;

                        if(voteElement.votingPerson.person.swiss != null){
                            nationality = voteElement.votingPerson.person.swiss.swissDomesticPerson;
                        }
                        if(voteElement.votingPerson.person.foreigner != null){
                            nationality = voteElement.votingPerson.person.foreigner.foreignerPerson;
                        }
                        if(voteElement.votingPerson.person.swissAbroad != null){
                            nationality = voteElement.votingPerson.person.swissAbroad.swissAbroadPerson;
                        }
                        if(nationality != null && nationality.extension != null && nationality.extension.municipalityRef != null){
                            municipality = Model.votingCardDelivery.extension.municipalities[nationality.extension.municipalityRef.Value];
                        }
                        if(nationality != null) {
                            personId = nationality.personIdentification.localPersonId.personId.ToString().PadLeft(11, '0');
                        }
                    }
                    @if(nationality != null && municipality != null){
                        <div class="printer-information"> $$VRSG$|eVot02|KU=@municipality.bfs|FO=BL100|DU=D|ID=@personId|PP=J|VE=@municipality.forwardDeliveryType|VA=@municipality.deliveryType|VR=@municipality.returnDeliveryType|BE=@municipality.attachmentStations|ZU=@voteElement.votingCardIndividualCodes.individualContestCodes[3].codeValue.ToString().Substring(0,22)|SP=@languageMap[nationality.languageOfCorrespondance]</div>
                    }
                    <div class="logo">
                        @{
                            String imageBase64src = string.Empty;
                            imageBase64src = "data:image/png;base64," + municipality.logo;
                        }
                        @if(municipality != null){
                            <img src=@imageBase64src>
                        }
                    </div>
                    <div class="blocker"></div>
                    @if(nationality != null && nationality.extension!= null){
                        <div class="reciver-address">
                            <span class="row">@nationality.extension.address.line1</span>
                            <span class="row">@nationality.extension.address.line2</span>
                            <span class="row">@nationality.extension.address.line3</span>
                            <span class="row">@nationality.extension.address.line4</span>
                            <span class="row">@nationality.extension.address.line5</span>
                            <span class="row">@nationality.extension.address.line6</span>
                            <span class="row">@nationality.extension.address.line7</span>
                        </div>
                    }
                    @if(voteElement != null && nationality != null){
                        <div class="birth-date">
                            <div>
                                @voteElement.votingCardIndividualCodes.individualContestCodes[3].codeValue
                            </div>
                            <div>
                                @nationality.personIdentification.dateOfBirth.yearMonthDay.ToString().Substring(6,4)
                            </div>
                        </div>
                    }
                    <div class="sender-address">
                        @if(municipality != null && municipality.returnDeliveryAddress != null){
                            <span class="row">@municipality.returnDeliveryAddress.addressField1</span>
                            <span class="row">Briefliche Stimmabgabe</span>
                            <span class="row">@municipality.returnDeliveryAddress.addressField2</span>
                            <span class="row">@municipality.returnDeliveryAddress.street</span>
                            <span class="row">@municipality.returnDeliveryAddress.plz @municipality.returnDeliveryAddress.municipality</span>
                            <span class="row">@municipality.returnDeliveryAddress.country</span>
                        }
                    </div>
                    <div class="sra-title">
                        <div><h2>Stimmrechtsausweis /</h2></div>
                        <div><h2>Carte de vote /</h2></div>
                        <div><h2>Carta di legittimazione di voto</h2></div>
                    </div>
                    <div class="sign">
                        <div class="color-green content sign-content">
                            <h3>Erklärung:</h3>
                            <div>Ich stimme brieflich. Die Stimmabgabe entspricht meinem Willen.</div>
                            <div>Je vote par correspondance. Ce vote correspond à ma volonté.</div>
                            <div>Voto per corrispondenza. Il voto corrisponde alla mia volontà.</div>
                            <div class="sign-line"> </div>
                            <span>(eigenhändige Unterschrift / signature manuscrite / firma autografa)</span>
                        </div>
                    </div>
                </div>

                <!-- TextBlocks-->
                @if(municipality != null && municipality.eTextBlocks != null){
                    @if(Model.votingCardDelivery.contestData != null && Model.votingCardDelivery.contestData.contest != null){
                        <div class="poll-date-title">
                            <div class="title">Urnengang vom @Model.votingCardDelivery.contestData.contest.contestDate.ToString().Split(' ')[0]</div>
                            <div class="title">Scrutin du @Model.votingCardDelivery.contestData.contest.contestDate.ToString().Split(' ')[0]</div>
                            <div class="title">Votazione del @Model.votingCardDelivery.contestData.contest.contestDate.ToString().Split(' ')[0]</div>
                        </div>
                    }
                    <div class="text-blocks  c@(municipality.eTextBlocks.columnQuantity)">
                        @{int textIndex = 0;}
                        @foreach (var blocks in municipality.eTextBlocks.columnQuantity.ToString().Split('-')){
                            <div class="text-blocks-column">
                                @foreach (var block in blocks.Split('_')){
                                    <div>
                                        @if(municipality.eTextBlocks.values.Count > textIndex){
                                            <div class="title">
                                                @municipality.eTextBlocks.values[textIndex].title
                                            </div>
                                            <div class="content">
                                                <div>
                                                    @{WriteLiteral(municipality.eTextBlocks.values[textIndex].text);}
                                                </div>
                                            </div>
                                        }
                                    </div>
                                    textIndex++;
                                }
                            </div>
                        }
                    </div>
                }

                <!-- e-Voting -->
                @if(voteElement.votingCardIndividualCodes != null){
                    <div class="content e-voting no-page-break-inside">
                        <div class="title">E-Voting / Vote électronique / Voto elettronico</div>

                        <div class="content">
                            @if (Model.votingCardDelivery != null && Model.votingCardDelivery.contestData != null && Model.votingCardDelivery.contestData.contest.eVotingPeriod != null)
                            {
                                string openingDateStr = municipality.pollOpening;
                                string closingDateStr = municipality.pollClosing;
                                DateTime openingDateTime = DateTime.ParseExact(openingDateStr, "dd.MM.yyyy HH:mm:ss", null);
                                DateTime closingTime = DateTime.ParseExact(closingDateStr, "dd.MM.yyyy HH:mm:ss", null);

                                System.Globalization.CultureInfo french = new System.Globalization.CultureInfo("fr");
                                System.Globalization.CultureInfo italian = new System.Globalization.CultureInfo("it");

                                string formattedOpeningDate = openingDateTime.ToString("dddd, dd.MM.yyyy, 'um' HH:mm");
                                string formattedOpeningDateFrench = openingDateTime.ToString("dddd, dd.MM.yyyy, à HH'h'mm",french);
                                string formattedOpeningDateItalian = openingDateTime.ToString("dddd, dd.MM.yyyy, alle HH:mm",italian);

                                string formattedClosingDate = closingTime.ToString("dddd, dd.MM.yyyy, 'um' HH:mm");
                                string formattedClosingDateFrench = closingTime.ToString("dddd, dd.MM.yyyy, à HH'h'mm",french);
                                string formattedClosingDateItalian = closingTime.ToString("dddd, dd.MM.yyyy, alle HH:mm",italian);

                                <div>Die <b>elektronische Urne </b> öffnet am  <b>@formattedOpeningDate Uhr</b> und schliesst am <b>@formattedClosingDate Uhr (MEZ)</b>.</div>
                                <div><b>L'urne électronique</b> ouvre le <b>@formattedOpeningDateFrench</b> et ferme le <b>@formattedClosingDateFrench (CET)</b>.</div>
                                <div><b>L'urna elettronica</b> apre il <b>@formattedOpeningDateItalian</b> e chiude il <b>@formattedClosingDateItalian (CET).</b></div>
                            }
                        </div>
                        <div class="poll-base-data content">
                            <div class="poll-base-data-table">
                                <table>
                                    <tr>
                                        <td>
                                            Web-Adresse:<br>
                                            Adresse internet:<br>
                                            Indirizzo Internet:

                                        </td>
                                        <th>https://sg.evoting.ch</th>
                                        <td><span class="qr-evoting"></span></td>
                                    </tr>
                                    <tr>
                                        <td>
                                            Initialisierungscode:<br>
                                            Code de initialisation:<br>
                                            Codice di inizializzazione:
                                        </td>
                                        <th>@System.Text.RegularExpressions.Regex.Replace(voteElement.votingCardIndividualCodes.individualContestCodes[0].codeValue.ToString(), ".{4}", "$0 \xa0\xa0")</th>
                                        <td class="icon-triangle"></td>
                                    </tr>
                                    <tr>
                                        <td>
                                            Bestätigungscode:<br>
                                            Code de confirmation:<br>
                                            Codice di conferma:
                                        </td>
                                        <th>@System.Text.RegularExpressions.Regex.Replace(voteElement.votingCardIndividualCodes.individualContestCodes[1].codeValue.ToString(), ".{4}", "$0 \xa0\xa0")</th>
                                        <td class="icon-pentagon"></td>
                                    </tr>
                                    <tr>
                                        <td>
                                            Finalisierungscode:<br>
                                            Code de finalisation:<br>
                                            Codice di finalizzazione:
                                        </td>
                                        <th>@System.Text.RegularExpressions.Regex.Replace(voteElement.votingCardIndividualCodes.individualContestCodes[2].codeValue.ToString(), ".{4}", "$0 \xa0\xa0")</th>
                                        <td class="icon-star"></td>
                                    </tr>
                                </table>
                            </div>
                            <div class="qr"> </div>
                        </div>
                    </div>
                    <div class="content e-voting no-page-break-inside">
                        <div class="content content-line">
                            <div><b>Helpdesk:</b> Montag - Freitag 08:30 Uhr - 11:30 Uhr und 14:00 Uhr - 16:00 Uhr, Tel.: +41 58 229 10 10, E-Mail: evoting@sg.ch</div>
                            <div><b>Helpdesk:</b> Du lundi au vendredi, de 08h30 à 11h30 et de 14h00 à 16h00, Tél.: +41 58 229 10 10, courriel: evoting@sg.ch</div>
                            <div><b>Helpdesk:</b> lunedì - venerdì dalle 08:30 alle 11:30 e dalle 14:00 alle 16:00, Tel.: +41 58 229 10 10, indirizzo mail: evoting@sg.ch</div>
                        </div>
                        <br />
                        <div class="content content-line">
                            <div>Die unbefugte oder mehrmalige Teilnahme an einer Wahl oder Abstimmung ist als Wahlfälschung strafbar (Art. 282 StGB).</div>
                            <div>Participer à une élection ou à une votation sans y être autorisé ou à plusieurs reprises est punissable en tant que fraude électorale (art. 282 CP).</div>
                            <div>Chi partecipa a un'elezione o a una votazione senza averne il diritto o vi partecipa più volte è punibile per frode elettorale (art. 282 CP).</div>
                        </div>
                        <br />
                        <div class="content content-line">
                        <div>Alle nachfolgenden Angaben dienen der Sicherheit von E-Voting. Wir empfehlen Ihnen, von diesen Kontrollelementen Gebrauch zu machen. Die nach dem Erfassen der Stimme auf dem Bildschirm angezeigten Prüfcodes müssen mit denjenigen auf Ihrem Stimmrechtsausweis übereinstimmen. Bitte wenden Sie sich an den Helpdesk, wenn falsche Codes angezeigt werden oder andere Prüfungen ein negatives Ergebnis aufweisen.</div>
                        <div>Tous les éléments suivants servent à vérifier la sécurité du vote électronique. Nous vous recommandons de faire usage de ces éléments de contrôle. Les codes de vérification qui s'affichent sur l'écran après la saisie du vote doivent correspondre à ceux qui figurent sur votre carte de vote. Merci de prendre contact avec le Helpdesk si de mauvais codes sont affichés ou si d'autres contrôles affichent un résultat négatif.</div>
                        <div>
                            Tutti i seguenti elementi servono per verificare la sicurezza del voto elettronico. Utilizzate questi elementi di verifica. I codici di verifica visualizzati sullo schermo dopo la registrazione del voto, devono corrispondere a quelli riportati sulla carta di legittimazione. Contattate l'helpdesk in caso che vengono visualizzati codici sbagliati o se ulteriori test risultano negativi.
                        </div>
                        </div>
                        <br />
                        <div class="content content-line">
                            <div>Wichtige Informationen zum Vorgehen und zur Verwendung der Prüfcodes finden Sie auf der Webseite des Kantons: https://e-voting.sg.ch. Im Fall von widersprüchlichen Angaben halten Sie sich bitte an die Informationen auf dem Stimmrechtsausweis, und nicht an die Informationen, die auf Webseiten angezeigt werden.</div>
                            <div>Des informations importantes sur les codes de vérification peuvent être consultées sur la page internet du canton: https://e-voting.sg.ch. En cas de doute, nous vous demandons de vous en tenir aux informations de la carte de vote et non à celles qui sont affichées sur les pages Internet.</div>
                            <div>Informazioni importanti sulla procedura e sui codici di verifica sono disponibili sul sito web del Cantone: https://e-voting.sg.ch. In caso di dubbio, è bene attenersi alle informazioni presenti sulla scheda di voto e non a quelle delle pagine Internet.</div>
                        </div>
                    </div>
                    <div class="content e-voting">
                        <div class="title fingerprint">
                            <div>
                                <label> <b>Fingerprint (SHA-256) des Zertifikats von https://sg.evoting.ch:</b></label>
                                <label> <b>Empreinte numérique (SHA-256) du certificat de https://sg.evoting.ch:</b></label>
                                <label> <b>Impronta digitale (SHA-256) del certificato da https://sg.evoting.ch:</b></label>
                            </div>
							
                            @if(municipality != null && municipality.vcteVotingFingerprint  != null){
                            <div class="fingerprint-code">
                                <span>@municipality.vcteVotingFingerprint</span>
                            </div>
                            }
                        </div>
                        <div class="poll-title title">
                            <h3>Prüfcodes / Codes de vérification / Codici di verifica @if(voteElement.poll != null && voteElement.poll.election != null){<text>Abstimmung</text>} <span class="rhombus">&#9670;</span></h3>
                        </div>
                        @if(voteElement.votingCardIndividualCodes != null && voteElement.votingCardIndividualCodes.vote != null && voteElement.votingCardIndividualCodes.vote.ToString().Length > 5){
                            @foreach (var vote in voteElement.votingCardIndividualCodes.vote){
                                <div class="poll-domain-of-influence content ">
                                    @vote.voteDescription[0].voteDescription / @vote.voteDescription[1].voteDescription / @vote.voteDescription[2].voteDescription
                                </div>
                                <div class="content">
                                    @if(vote.ballot != null ){
                                        @foreach (var ballot in vote.ballot){
                                            @* -------question -------*@
                                            @if(@ballot.standardBallot != null){
                                                <div  class="deals">
                                                    <span>Vorlage @ballot.ballotPosition / Objet @ballot.ballotPosition / Oggetto @ballot.ballotPosition</span>
                                                    @foreach (var answer in ballot.standardBallot.questionInformation.answerOption){
                                                        <span class="textRight">
                                                                @if(@answer.answerTextInformation[0].answerText.ToString().Length > 6 ){
                                                                    <span> @answer.answerTextInformation[0].answerText / </span><br> <span>@answer.answerTextInformation[1].answerText /</span><br><span> @answer.answerTextInformation[2].answerText</span>
                                                                }
                                                                @if(@answer.answerTextInformation[0].answerText.ToString().Length <= 6 ){
                                                                    <span> @answer.answerTextInformation[0].answerText / </span><span>@answer.answerTextInformation[1].answerText /</span><span> @answer.answerTextInformation[2].answerText</span>
                                                                }
                                                                <b> @answer.individualVoteVerificationCode</b> 	
                                                        </span>	
                                                    }
                                                </div>
                                            }
                                            @* -------tieBreakInformation -------*@
                                            @if(ballot.variantBallot != null){
                                                @foreach (var questionInformation in ballot.variantBallot.questionInformation){
                                                    <div  class="deals">
                                                        <span>Vorlage @questionInformation.ballotQuestionNumber / Objet @questionInformation.ballotQuestionNumber / Oggetto @questionInformation.ballotQuestionNumber</span>
                                                        @foreach (var answer in questionInformation.answerOption){
                                                        <span class="textRight">
                                                                @if(@answer.answerTextInformation[0].answerText.ToString().Length > 6 ){
                                                                    <span> @answer.answerTextInformation[0].answerText / </span><br> <span>@answer.answerTextInformation[1].answerText /</span><br><span> @answer.answerTextInformation[2].answerText</span>
                                                                }
                                                                @if(@answer.answerTextInformation[0].answerText.ToString().Length <= 6 ){
                                                                    <span> @answer.answerTextInformation[0].answerText / </span><span>@answer.answerTextInformation[1].answerText /</span><span> @answer.answerTextInformation[2].answerText</span>
                                                                }
                                                                <b> @answer.individualVoteVerificationCode</b> 	
                                                            </span>	
                                                        }
                                                    </div>
                                                }
                                            }
                                            @if(ballot.variantBallot != null && ballot.variantBallot.tieBreakInformation != null){
                                                @foreach (var tieBreakInformation in ballot.variantBallot.tieBreakInformation){
                                                    <div class="deals">
                                                        <span>Vorlage @tieBreakInformation.tieBreakQuestionNumber / Objet @tieBreakInformation.tieBreakQuestionNumber / Oggetto @tieBreakInformation.tieBreakQuestionNumber</span>
                                                        @foreach (var answer in tieBreakInformation.answerOption){
                                                            <span class="textRight">
                                                                @if(@answer.answerTextInformation[0].answerText.ToString().Length > 6 ){
                                                                    <span> @answer.answerTextInformation[0].answerText / </span><br> <span>@answer.answerTextInformation[1].answerText /</span><br><span> @answer.answerTextInformation[2].answerText</span>
                                                                }
                                                                @if(@answer.answerTextInformation[0].answerText.ToString().Length <= 6 ){
                                                                    <span> @answer.answerTextInformation[0].answerText / </span><span>@answer.answerTextInformation[1].answerText /</span><span> @answer.answerTextInformation[2].answerText</span>
                                                                }
                                                                <b> @answer.individualVoteVerificationCode</b> 	
                                                            </span>	
                                                        }
                                                    </div>
                                                }
                                            }
                                        }
                                    }
                                </div>
                            }
                        }

                        @if(@voteElement.votingCardIndividualCodes != null && voteElement.votingCardIndividualCodes.electionGroupBallot != null){
                            @* ------------MajorzWahl-2--------------*@
                            @foreach (var electionGroupBallot in voteElement.votingCardIndividualCodes.electionGroupBallot){
                                @foreach (var electionInformation in electionGroupBallot.electionInformation){
                                    @if (electionInformation.candidate != null && electionInformation.list == null){
                                        <div class="poll-title title">
                                            @electionInformation.election.electionDescription[0].electionDescription
                                        </div>
                                        <div class="content">
                                            @foreach (var candidate in electionInformation.candidate){
                                                @foreach (var candidateReference in candidate.candidateReference){
                                                    <div class="deals">
                                                        <span>@candidateReference.candidateTextOnPosition[0].candidateText</span>
                                                        <span><b>@candidateReference.individualCandidateVerificationCode[0].codeValue</b></span>
                                                    </div>
                                                }
                                            }
                                            @if(@electionInformation.writeInCodes != null){
                                                @foreach (var writeInCodes in electionInformation.writeInCodes){
                                                    <div class="deals">
                                                        <span>@writeInCodes.writeInCodeDesignation[0].codeDesignationText</span>
                                                        <span><b>@writeInCodes.individualWriteInVerificationCode</b></span>
                                                    </div>
                                                }
                                            }
                                        </div>
                                    }
                                }
                            }

                            @* ------------Proporz-1---------------*@
                            @foreach (var electionGroupBallot in voteElement.votingCardIndividualCodes.electionGroupBallot){
                                @foreach (var electionInformation in electionGroupBallot.electionInformation){
                                    @if (electionInformation.list != null && electionInformation.candidate == null){
                                        @if (electionInformation.election != null && electionInformation.election.electionDescription != null){
                                            <div class="poll-title title ">
                                                @electionInformation.election.electionDescription[0].electionDescription
                                            </div>
                                        }
                                        <div class="content">
                                            <div><b>Codes der Kandidierenden / Codes des candidats / Codici dei candidati</b></div>
                                            <div>"1x" = einmal genannt / inscrit une fois / indicato una volta</div>
                                            <div>"2x" = zweimal genannt (kumuliert) / inscrit deux fois (cumul) / indicato due volte (cumulativo)</div>
                                            <div>Die Codes für einen Stimmzettel ohne Listenbezeichnung und jene für leere Linien auf einem Stimmzettel finden sich in der letzten Tabelle (Liste 99).</div>
                                            <div>Les codes pour un bulletin sans choix de liste ou pour chaque ligne vide dans le bulletin se trouvent dans le dernier tableau (liste 99).</div>
                                            <div>Nell'ultima tabella (lista 99) si trovano i codici per una scheda senza intestazione della lista e i codici per indicare una riga vuota sulla scheda.</div>
                                        </div>
                                        <div class="proporz">
                                            @foreach (var list in electionInformation.list){
                                                <div class="proporz-item">
                                                    <table>
                                                        <tr class="color-green content">
                                                            <td>Liste</td>
                                                            <td></td>
                                                            <td class="code">Code</td>
                                                        </tr>
                                                        <tr class="content">
                                                            <td>@list.listIndentureNumber</td>
                                                            <td></td>
                                                            <td class="code"><b>@list.individualListVerificationCodes[0].codeValue</b></td>
                                                        </tr>
                                                        <tr class="color-green content">
                                                            <td>Kand.Nr</td>
                                                            <td  class="code">1x</td>
                                                            <td  class="code">2x</td>

                                                        </tr>
                                                        @foreach (var candidatePosition in list.candidatePosition){
                                                            <tr class="content">
                                                                <td>@candidatePosition.candidateReferenceOnPosition</td>
                                                                @foreach (var individualCandidateVerificationCode in candidatePosition.individualCandidateVerificationCode){
                                                                    <td  class="code"><b>@individualCandidateVerificationCode.codeValue</b> </td>
                                                                }
                                                            </tr>
                                                        }
                                                    </table>
                                                </div>
                                            }
                                        </div>
                                    }
                                }
                            }
                        }
                    </div>
                }
            </div>
        }
    </body>
</html>
